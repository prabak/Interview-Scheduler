<!DOCTYPE html>
<html>
<head>
  <title>Appointment</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>


<body>
    <div id="page">
      <div id="sidebar">
      	<%= yield :sidebar %>
      </div>

      <div id="content">

		<div id="header">
          <%= yield :center_top %>
        </div>

        <div id="about">
          <%= yield :center_middle %>
        </div>

        <div id="getting-started">
			<% if flash[:notice] %>
            	<div id="notice"><%= flash[:notice] %></div>
			<% end %>
    		<% if flash[:error] %>
				<div id="error"><%= flash[:error] %></div>
			<% end %>

        	<%= yield :center_bottom %>
		</div>
      </div>

      <div id="footer">&nbsp;</div>
    </div>


</body>
</html>